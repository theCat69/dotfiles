[core]
  editor = vim
  pager = batcat --pager=never -p
  longpaths = true
[pull]
  rebase = false
[fetch]
  prune = false
[rebase]
  autoStash = false
[merge]
  conflictstyle = diff3
  tool = nvimdiff
[diff]
  colorMoved = default
[init]
  defaultBranch = main
[mergetool]
  keepBackup = false
